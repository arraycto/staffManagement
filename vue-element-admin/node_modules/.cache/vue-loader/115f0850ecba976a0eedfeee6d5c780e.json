n      newPasswordone: \"\",\n      newPasswordtwo: \"\",\n      saveBtnDisabled: false,\n    };\n  },\n  created() {\n     if (this.$route.params && this.$route.params.id) {\n      } else {\n        //清空表单\n        this.$router.push({ path: \"savePassword/validationPassword\" });\n      }\n  },\n  methods: {\n    updateUserPassword() {\n      if (this.newPasswordone.length >= 6) {\n        if (this.newPasswordone === this.newPasswordtwo) {\n          clerk\n            .putPassword(this.newPasswordone)\n            .then((response) => {\n              this.$message({\n                type: \"success\",\n                message: \"修改成功!\",\n              });\n              this.$router.push({ path: \"/\" });\n            })\n            .catch((error) => {});\n        } else {\n          this.$message({\n            type: \"warning\",\n            message: \"两次输入密码必须相同\",\n          });\n        }\n      } else {\n        this.$message({\n          type: \"warning\",\n          message: \"长度必须大于六位\",\n        });\n      }\n    },\n  },\n};\n</script>\n\n<style>\n</style>\n\n"]}]}
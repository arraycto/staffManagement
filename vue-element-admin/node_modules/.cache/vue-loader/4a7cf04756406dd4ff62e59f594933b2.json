{"remainingRequest":"/Users/xixi/staff_management/vue-element-admin/node_modules/vue-loader/lib/index.js??vue-loader-options!/Users/xixi/staff_management/vue-element-admin/src/views/attendance/listAttendanceThrough.vue?vue&type=script&lang=js&","dependencies":[{"path":"/Users/xixi/staff_management/vue-element-admin/src/views/attendance/listAttendanceThrough.vue","mtime":1608726456323},{"path":"/Users/xixi/staff_management/vue-element-admin/node_modules/cache-loader/dist/cjs.js","mtime":1608726442352},{"path":"/Users/xixi/staff_management/vue-element-admin/node_modules/babel-loader/lib/index.js","mtime":1608726441374},{"path":"/Users/xixi/staff_management/vue-element-admin/node_modules/cache-loader/dist/cjs.js","mtime":1608726442352},{"path":"/Users/xixi/staff_management/vue-element-admin/node_modules/vue-loader/lib/index.js","mtime":1608726455182}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["listAttendanceThrough.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA+FA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"listAttendanceThrough.vue","sourceRoot":"src/views/attendance","sourcesContent":["<template>\n  <div class=\"app-container\">\n    <!--查询表单-->\n    <el-form :inline=\"true\" class=\"demo-form-inline\">\n      <el-form-item>\n        <el-input\n          v-model=\"attendanceQueryVo.clerkName\"\n          placeholder=\"员工名称\"\n        />\n      </el-form-item>\n      <el-form-item>\n        <el-input\n          v-model=\"attendanceQueryVo.departmentName\"\n          placeholder=\"部门名称\"\n        />\n      </el-form-item>\n      <el-form-item>\n        <el-select\n          clearable\n          v-model=\"attendanceQueryVo.status\"\n          placeholder=\"请选择考勤类型\"\n        >\n          <el-option\n            v-for=\"item in options\"\n            :key=\"item.value\"\n            :label=\"item.label\"\n            :value=\"item.value\"\n          >\n          </el-option>\n        </el-select>\n      </el-form-item>\n\n      <el-button\n        type=\"primary\"\n        icon=\"el-icon-search\"\n        @click=\"getAttendanceListPage()\"\n        >查询</el-button\n      >\n      <el-button type=\"default\" @click=\"resetData()\">清空</el-button>\n      <el-button\n        :loading=\"downloadLoading\"\n        style=\"margin: 0 0 20px 20px\"\n        type=\"primary\"\n        icon=\"el-icon-document\"\n        @click=\"handleDownload\"\n      >\n        下载Excel\n      </el-button>\n    </el-form>\n    <el-table\n      :data=\"list\"\n      element-loading-text=\"数据加载中\"\n      fit\n      highlight-current-row\n    >\n      <el-table-column label=\"序号\" width=\"70\" align=\"center\">\n        <template slot-scope=\"scope\">{{\n          (page - 1) * limit + scope.$index + 1\n        }}</template>\n      </el-table-column>\n      <el-table-column prop=\"id\" sortable label=\"编号\" width=\"200\" />\n      <el-table-column prop=\"time\" sortable label=\"日期\" width=\"200\" />\n      <el-table-column prop=\"status\" label=\"类型\" />\n      <el-table-column prop=\"clerkId\" sortable label=\"员工姓名\" />\n      <el-table-column prop=\"departmentId\" sortable label=\"员工部门\" />\n      <el-table-column\n        v-if=\"isAdmin == true\"\n        label=\"操作\"\n        width=\"200\"\n        align=\"center\"\n      >\n        <template slot-scope=\"scope\">\n          <el-button\n            type=\"danger\"\n            size=\"mini\"\n            icon=\"el-icon-delete\"\n            @click=\"deleteAttendanceById(scope.row.id)\"\n            >删除</el-button\n          >\n        </template>\n      </el-table-column>\n    </el-table>\n    <!-- 分页 -->\n    <el-pagination\n      :current-page=\"page\"\n      :page-size=\"limit\"\n      :total=\"total\"\n      style=\"padding: 30px 0; text-align: center\"\n      layout=\"total, prev, pager, next, jumper\"\n      @current-change=\"getAttendanceListPage\"\n    />\n  </div>\n</template>\n\n<script>\nimport attendance from \"@/api/attendance\";\nimport { mapGetters } from \"vuex\";\nexport default {\n  data() {\n    return {\n      downloadLoading: false,\n      //            事假1,迟到2,早退3,病假4,旷工5,休补6\n      options: [\n        {\n          value: \"1\",\n          label: \"事假\",\n        },\n        {\n          value: \"2\",\n          label: \"迟到\",\n        },\n        {\n          value: \"3\",\n          label: \"早退\",\n        },\n        {\n          value: \"4\",\n          label: \"病假\",\n        },\n        {\n          value: \"5\",\n          label: \"旷工\",\n        },\n        {\n          value: \"6\",\n          label: \"休补\",\n        },\n      ],\n      list: null, //查询之后接口返回集合\n      page: 1, //当前页\n      limit: 6, //每页记录数\n      total: 0, //总记录数\n      attendanceQueryVo: {\n        audit: \"1\",\n      },\n      isAdmin: false,\n    };\n  },\n  computed: {\n    ...mapGetters([\"roles\"]),\n  },\n  created() {\n    if (this.roles.includes(\"管理员\")) {\n      this.isAdmin = true;\n    }\n    this.getAttendanceListPage();\n  },\n  methods: {\n    handleDownload() {\n      this.downloadLoading = true;\n      import(\"@/vendor/Export2Excel\").then((excel) => {\n        const tHeader = [\"编号\", \"员工姓名\", \"部门\", \"时间\", \"类型\"];\n        const filterVal = [\"id\", \"clerkId\", \"departmentId\", \"time\", \"status\"];\n        const list = this.list;\n        const data = this.formatJson(filterVal, list);\n        excel.export_json_to_excel({\n          header: tHeader,\n          data,\n          filename: \"考勤信息\",\n          autoWidth: true,\n          bookType: \"xlsx\",\n        });\n        this.downloadLoading = false;\n      });\n    },\n    formatJson(filterVal, jsonData) {\n      return jsonData.map((v) =>\n        filterVal.map((j) => {\n          if (j === \"timestamp\") {\n            return parseTime(v[j]);\n          } else {\n            return v[j];\n          }\n        })\n      );\n    },\n    resetData() {\n      //清空的方法\n      //表单数据清空\n      this.attendanceQueryVo = {};\n      //查询所有\n      this.getAttendanceListPage();\n    },\n    //页面渲染之后执行，一般用于创建具体方法\n    //列表方法\n    getAttendanceListPage(page = 1) {\n      this.page = page;\n      attendance\n        .getAttendanceListPage(this.page, this.limit, this.attendanceQueryVo)\n        .then((response) => {\n          //请求成功\n          //response接口返回的数据\n          this.list = response.data.attendanceList;\n          this.total = response.data.total;\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    },\n    resetData() {\n      this.getAttendanceListPage();\n    },\n    deleteAttendanceById(id) {\n      //删除功能\n      this.$confirm(\"此操作将永久删除该记录, 是否继续?\", \"提示\", {\n        confirmButtonText: \"确定\",\n        cancelButtonText: \"取消\",\n        type: \"warning\",\n      }).then(() => {\n        attendance\n          .deleteAttendanceById(id)\n          .then((response) => {\n            //删除成功\n            //提示信息\n            this.$message({\n              type: \"success\",\n              message: \"删除成功!\",\n            });\n            //回到列表\n            this.getAttendanceListPage();\n          })\n          .catch((error) => {\n            //提示信息\n            this.$message({\n              type: \"warning\",\n              message: \"删除失败!\",\n            });\n          });\n      });\n    },\n  },\n};\n</script>\n\n"]}]}
1CA;AA2CA,EAAA,OAAA;AACA,IAAA,SADA,uBACA;AACA;AACA;AACA,WAAA,iBAAA,GAAA,EAAA,CAHA,CAIA;;AACA,WAAA,qBAAA;AACA,KAPA;AAQA;AACA;AACA,IAAA,qBAVA,mCAUA;AAAA;;AAAA,UAAA,IAAA,uEAAA,CAAA;AACA,WAAA,IAAA,GAAA,IAAA;AACA,MAAA,UAAA,CACA,qBADA,CAEA,KAAA,IAFA,EAGA,KAAA,KAHA,EAIA,KAAA,iBAJA,EAMA,IANA,CAMA,UAAA,QAAA,EAAA;AACA;AACA;AACA,QAAA,KAAA,CAAA,IAAA,GAAA,QAAA,CAAA,IAAA,CAAA,cAAA;AACA,QAAA,KAAA,CAAA,KAAA,GAAA,QAAA,CAAA,IAAA,CAAA,KAAA;AACA,OAXA,EAYA,KAZA,CAYA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OAdA;AAeA;AA3BA,iEA4BA;AACA,SAAA,qBAAA;AACA,GA9BA,mFA+BA,EA/BA,EA+BA;AAAA;;AACA;AACA,SAAA,QAAA,CAAA,oBAAA,EAAA,IAAA,EAAA;AACA,MAAA,iBAAA,EAAA,IADA;AAEA,MAAA,gBAAA,EAAA,IAFA;AAGA,MAAA,IAAA,EAAA;AAHA,KAAA,EAIA,IAJA,CAIA,YAAA;AACA,MAAA,UAAA,CACA,oBADA,CACA,EADA,EAEA,IAFA,CAEA,UAAA,QAAA,EAAA;AACA;AACA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA;AACA,UAAA,IAAA,EAAA,SADA;AAEA,UAAA,OAAA,EAAA;AAFA,SAAA,EAHA,CAOA;;;AACA,QAAA,MAAA,CAAA,qBAAA;AACA,OAXA,EAYA,KAZA,CAYA,UAAA,KAAA,EAAA;AACA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA;AACA,UAAA,IAAA,EAAA,SADA;AAEA,UAAA,OAAA,EAAA;AAFA,SAAA;AAIA,OAlBA;AAmBA,KAxBA;AAyBA,GA1DA;AA3CA,CAAA","sourcesContent":["<template>\n  <div class=\"app-container\">\n    <!--查询表单-->\n    <el-form :inline=\"true\" class=\"demo-form-inline\">\n      <el-form-item>\n        <el-input\n          v-model=\"attendanceQueryVo.clerkName\"\n          placeholder=\"员工名称\"\n        />\n      </el-form-item>\n      <el-form-item>\n        <el-input\n          v-model=\"attendanceQueryVo.departmentName\"\n          placeholder=\"部门名称\"\n        />\n      </el-form-item>\n      <el-form-item>\n        <el-select\n          clearable\n          v-model=\"attendanceQueryVo.status\"\n          placeholder=\"请选择考勤类型\"\n        >\n          <el-option\n            v-for=\"item in options\"\n            :key=\"item.value\"\n            :label=\"item.label\"\n            :value=\"item.value\"\n          >\n          </el-option>\n        </el-select>\n      </el-form-item>\n\n      <el-button\n        type=\"primary\"\n        icon=\"el-icon-search\"\n        @click=\"getAttendanceListPage()\"\n        >查询</el-button\n      >\n      <el-button type=\"default\" @click=\"resetData()\">清空</el-button>\n    </el-form>\n    <el-table\n      :data=\"list\"\n      element-loading-text=\"数据加载中\"\n      fit\n      highlight-current-row\n    >\n      <el-table-column label=\"序号\" width=\"70\" align=\"center\">\n        <template slot-scope=\"scope\">{{\n          (page - 1) * limit + scope.$index + 1\n        }}</template>\n      </el-table-column>\n      <el-table-column prop=\"id\" sortable label=\"编号\" width=\"200\" />\n      <el-table-column prop=\"time\" sortable label=\"日期\" width=\"200\" />\n      <el-table-column prop=\"status\" label=\"类型\" />\n      <el-table-column prop=\"clerkId\" sortable label=\"员工姓名\" />\n      <el-table-column prop=\"departmentId\" sortable label=\"员工部门\" />\n      <el-table-column label=\"操作\" width=\"200\" align=\"center\">\n        <template slot-scope=\"scope\">\n          <router-link\n            :to=\"'/attendance/updateAttendance/' + scope.row.id\"\n          >\n            <el-button type=\"primary\" size=\"mini\" icon=\"el-icon-edit\"\n              >修改并重新提交</el-button\n            >\n          </router-link>\n          <el-button\n            type=\"danger\"\n            size=\"mini\"\n            icon=\"el-icon-delete\"\n            @click=\"deleteAttendanceById(scope.row.id)\"\n            >删除</el-button\n          >\n        </template>\n      </el-table-column>\n    </el-table>\n    <!-- 分页 -->\n    <el-pagination\n      :current-page=\"page\"\n      :page-size=\"limit\"\n      :total=\"total\"\n      style=\"padding: 30px 0; text-align: center\"\n      layout=\"total, prev, pager, next, jumper\"\n      @current-change=\"getAttendanceListPage\"\n    />\n  </div>\n</template>\n\n<script>\nimport attendance from \"@/api/attendance\";\nexport default {\n  data() {\n    return {\n      //            事假1,迟到2,早退3,病假4,旷工5,休补6\n      options: [\n        {\n          value: \"1\",\n          label: \"事假\",\n        },\n        {\n          value: \"2\",\n          label: \"迟到\",\n        },\n        {\n          value: \"3\",\n          label: \"早退\",\n        },\n        {\n          value: \"4\",\n          label: \"病假\",\n        },\n        {\n          value: \"5\",\n          label: \"旷工\",\n        },\n        {\n          value: \"6\",\n          label: \"休补\",\n        },\n      ],\n      list: null, //查询之后接口返回集合\n      page: 1, //当前页\n      limit: 6, //每页记录数\n      total: 0, //总记录数\n      attendanceQueryVo: {\n        audit:'2'\n      },\n    };\n  },\n  created() {\n    //页面渲染之前执行，一般用于调用methods中定义的方法\n    this.getAttendanceListPage();\n  },\n  methods: {\n    resetData() {\n      //清空的方法\n      //表单数据清空\n      this.attendanceQueryVo = {};\n      //查询所有\n      this.getAttendanceListPage();\n    },\n    //页面渲染之后执行，一般用于创建具体方法\n    //列表方法\n    getAttendanceListPage(page = 1) {\n      this.page = page;\n      attendance\n        .getAttendanceListPage(\n          this.page,\n          this.limit,\n          this.attendanceQueryVo\n        )\n        .then((response) => {\n          //请求成功\n          //response接口返回的数据\n          this.list = response.data.attendanceList;\n          this.total = response.data.total;\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    },\n    resetData() {\n      this.getAttendanceListPage();\n    },\n    deleteAttendanceById(id) {\n      //删除功能\n      this.$confirm(\"此操作将永久删除该记录, 是否继续?\", \"提示\", {\n        confirmButtonText: \"确定\",\n        cancelButtonText: \"取消\",\n        type: \"warning\",\n      }).then(() => {\n        attendance\n          .deleteAttendanceById(id)\n          .then((response) => {\n            //删除成功\n            //提示信息\n            this.$message({\n              type: \"success\",\n              message: \"删除成功!\",\n            });\n            //回到列表\n            this.getAttendanceListPage();\n          })\n          .catch((error) => {\n            //提示信息\n            this.$message({\n              type: \"warning\",\n              message: \"删除失败!\",\n            });\n          });\n      });\n    },\n  },\n};\n</script>\n\n"],"sourceRoot":"src/views/attendance"}]}
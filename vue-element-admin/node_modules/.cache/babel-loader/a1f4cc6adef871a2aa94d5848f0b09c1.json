A,EAAA,IAAA,CAAA;;AACA,QAAA,KAAA,CAAA,oBAAA,CAAA;AACA,UAAA,MAAA,EAAA,OADA;AAEA,UAAA,IAAA,EAAA,IAFA;AAGA,UAAA,QAAA,EAAA,MAHA;AAIA,UAAA,SAAA,EAAA,IAJA;AAKA,UAAA,QAAA,EAAA;AALA,SAAA;AAOA,QAAA,KAAA,CAAA,eAAA,GAAA,KAAA;AACA,OAjCA;AAkCA,KArCA;AAsCA,IAAA,UAtCA,sBAsCA,SAtCA,EAsCA,QAtCA,EAsCA;AACA,aAAA,QAAA,CAAA,GAAA,CAAA,UAAA,CAAA;AAAA,eACA,SAAA,CAAA,GAAA,CAAA,UAAA,CAAA,EAAA;AACA,cAAA,CAAA,KAAA,WAAA,EAAA;AACA,mBAAA,SAAA,CAAA,CAAA,CAAA,CAAA,CAAA,CAAA;AACA,WAFA,MAEA;AACA,mBAAA,CAAA,CAAA,CAAA,CAAA;AACA;AACA,SANA,CADA;AAAA,OAAA,CAAA;AASA,KAhDA;AAiDA,IAAA,SAjDA,uBAiDA;AACA;AACA;AACA,WAAA,YAAA,GAAA,EAAA,CAHA,CAIA;;AACA,WAAA,gBAAA;AACA,KAvDA;AAwDA;AACA;AACA,IAAA,gBA1DA,8BA0DA;AAAA;;AAAA,UAAA,IAAA,uEAAA,CAAA;AACA,WAAA,IAAA,GAAA,IAAA;AACA,MAAA,KAAA,CACA,gBADA,CACA,KAAA,IADA,EACA,KAAA,KADA,EACA,KAAA,YADA,EAEA,IAFA,CAEA,UAAA,QAAA,EAAA;AACA;AACA;AACA,QAAA,MAAA,CAAA,IAAA,GAAA,QAAA,CAAA,IAAA,CAAA,SAAA;AACA,QAAA,MAAA,CAAA,KAAA,GAAA,QAAA,CAAA,IAAA,CAAA,KAAA;AACA,OAPA,EAQA,KARA,CAQA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OAVA;AAWA;AAvEA,iEAwEA;AACA,SAAA,gBAAA;AACA,GA1EA,yEA2EA,EA3EA,EA2EA;AAAA;;AACA;AACA,SAAA,QAAA,CAAA,oBAAA,EAAA,IAAA,EAAA;AACA,MAAA,iBAAA,EAAA,IADA;AAEA,MAAA,gBAAA,EAAA,IAFA;AAGA,MAAA,IAAA,EAAA;AAHA,KAAA,EAIA,IAJA,CAIA,YAAA;AACA,MAAA,KAAA,CACA,eADA,CACA,EADA,EAEA,IAFA,CAEA,UAAA,QAAA,EAAA;AACA;AACA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA;AACA,UAAA,IAAA,EAAA,SADA;AAEA,UAAA,OAAA,EAAA;AAFA,SAAA,EAHA,CAOA;;;AACA,QAAA,MAAA,CAAA,gBAAA;AACA,OAXA,EAYA,KAZA,CAYA,UAAA,KAAA,EAAA;AACA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA;AACA,UAAA,IAAA,EAAA,SADA;AAEA,UAAA,OAAA,EAAA;AAFA,SAAA;AAIA,OAlBA;AAmBA,KAxBA;AAyBA,GAtGA;AAfA,CAAA","sourcesContent":["<template>\n  <div class=\"app-container\">\n    <!--查询表单-->\n    <el-form :inline=\"true\" class=\"demo-form-inline\">\n      <el-form-item>\n        <el-input v-model=\"clerkQueryVo.name\" placeholder=\"员工名称\" />\n      </el-form-item>\n      <el-form-item>\n        <el-input v-model=\"clerkQueryVo.position\" placeholder=\"员工职位\" />\n      </el-form-item>\n      <el-form-item>\n        <el-input\n          v-model=\"clerkQueryVo.departmentName\"\n          placeholder=\"部门名称\"\n        />\n      </el-form-item>\n\n      <el-button\n        type=\"primary\"\n        icon=\"el-icon-search\"\n        @click=\"getClerkListPage()\"\n        >查询</el-button\n      >\n      <el-button type=\"default\" @click=\"resetData()\">清空</el-button>\n      <el-button\n        :loading=\"downloadLoading\"\n        style=\"margin: 0 0 20px 20px\"\n        type=\"primary\"\n        icon=\"el-icon-document\"\n        @click=\"handleDownload\"\n      >\n        下载Excel\n      </el-button>\n    </el-form>\n    <el-table\n      :data=\"list\"\n      element-loading-text=\"数据加载中\"\n      fit\n      highlight-current-row\n    >\n      <el-table-column type=\"expand\">\n        <template slot-scope=\"props\">\n          <el-form label-position=\"left\" inline class=\"demo-table-expand\">\n            <el-form-item label=\"照片\">\n              <el-image\n                style=\"width: 100px; height: 100px\"\n                :src=\"props.row.photo\"\n                fit=\"contain\"\n              /> </el-form-item\n            ><br />\n            <el-form-item label=\"员工名称\">\n              <span>{{ props.row.name }}</span>\n            </el-form-item>\n            <br />\n            <el-form-item label=\"员工性别\">\n              <span>{{ props.row.sex == 1 ? \"男\" : \"女\" }}</span> </el-form-item\n            ><br />\n            <el-form-item label=\"员工手机号码\">\n              <span>{{ props.row.phoneNumber }}</span> </el-form-item\n            ><br />\n            <el-form-item label=\"员工身份证\">\n              <span>{{ props.row.idCard }}</span> </el-form-item\n            ><br />\n            <el-form-item label=\"员工住址\">\n              <span>{{ props.row.address }}</span>\n            </el-form-item>\n          </el-form>\n        </template>\n      </el-table-column>\n\n      <el-table-column label=\"序号\" width=\"70\" align=\"center\">\n        <template slot-scope=\"scope\">{{\n          (page - 1) * limit + scope.$index + 1\n        }}</template>\n      </el-table-column>\n\n      <el-table-column prop=\"id\" sortable label=\"员工编号\" width=\"200\" />\n      <el-table-column prop=\"name\" sortable label=\"员工姓名\" width=\"100\" />\n      <el-table-column prop=\"position\" sortable label=\"员工职位\" />\n      <el-table-column\n        prop=\"departmentId\"\n        sortable\n        label=\"员工部门\"\n        width=\"100\"\n      />\n      <el-table-column label=\"操作\" width=\"200\" align=\"center\">\n        <template slot-scope=\"scope\">\n          <router-link :to=\"'/clerk/updateClerk/' + scope.row.id\">\n            <el-button type=\"primary\" size=\"mini\" icon=\"el-icon-edit\"\n              >修改</el-button\n            >\n          </router-link>\n          <el-button\n            type=\"danger\"\n            size=\"mini\"\n            icon=\"el-icon-delete\"\n            @click=\"deleteClerkById(scope.row.id)\"\n            >员工离职</el-button\n          >\n        </template>\n      </el-table-column>\n    </el-table>\n    <!-- 分页 -->\n    <el-pagination\n      :current-page=\"page\"\n      :page-size=\"limit\"\n      :total=\"total\"\n      style=\"padding: 30px 0; text-align: center\"\n      layout=\"total, prev, pager, next, jumper\"\n      @current-change=\"getClerkListPage\"\n    />\n  </div>\n</template>\n\n<script>\nimport clerk from \"@/api/clerk\";\nexport default {\n  data() {\n    return {\n      downloadLoading: false,\n      list: null, //查询之后接口返回集合\n      page: 1, //当前页\n      limit: 6, //每页记录数\n      total: 0, //总记录数\n      clerkQueryVo: {},\n    };\n  },\n  created() {\n    //页面渲染之前执行，一般用于调用methods中定义的方法\n    this.getClerkListPage();\n  },\n  methods: {\n    handleDownload() {\n      this.downloadLoading = true;\n      import(\"@/vendor/Export2Excel\").then((excel) => {\n        const tHeader = [\n          \"编号\",\n          \"姓名\",\n          \"照片\",\n          \"性别\",\n          \"部门\",\n          \"职位\",\n          \"手机号\",\n          \"身份证号\",\n          \"住址\",\n        ];\n        const filterVal = [\n          \"id\",\n          \"name\",\n          \"photo\",\n          \"sex\",\n          \"departmentId\",\n          \"position\",\n          \"phoneNumber\",\n          \"idCard\",\n          \"address\",\n        ];\n        const list = this.list;\n        const data = this.formatJson(filterVal, list);\n        excel.export_json_to_excel({\n          header: tHeader,\n          data,\n          filename: \"员工信息\",\n          autoWidth: true,\n          bookType: \"xlsx\",\n        });\n        this.downloadLoading = false;\n      });\n    },\n    formatJson(filterVal, jsonData) {\n      return jsonData.map((v) =>\n        filterVal.map((j) => {\n          if (j === \"timestamp\") {\n            return parseTime(v[j]);\n          } else {\n            return v[j];\n          }\n        })\n      );\n    },\n    resetData() {\n      //清空的方法\n      //表单数据清空\n      this.clerkQueryVo = {};\n      //查询所有\n      this.getClerkListPage();\n    },\n    //页面渲染之后执行，一般用于创建具体方法\n    //列表方法\n    getClerkListPage(page = 1) {\n      this.page = page;\n      clerk\n        .getClerkListPage(this.page, this.limit, this.clerkQueryVo)\n        .then((response) => {\n          //请求成功\n          //response接口返回的数据\n          this.list = response.data.clerkList;\n          this.total = response.data.total;\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    },\n    resetData() {\n      this.getClerkListPage();\n    },\n    deleteClerkById(id) {\n      //删除功能\n      this.$confirm(\"此操作将永久删除该记录, 是否继续?\", \"提示\", {\n        confirmButtonText: \"确定\",\n        cancelButtonText: \"取消\",\n        type: \"warning\",\n      }).then(() => {\n        clerk\n          .deleteClerkById(id)\n          .then((response) => {\n            //删除成功\n            //提示信息\n            this.$message({\n              type: \"success\",\n              message: \"删除成功!\",\n            });\n            //回到列表\n            this.getClerkListPage();\n          })\n          .catch((error) => {\n            //提示信息\n            this.$message({\n              type: \"warning\",\n              message: \"删除失败!\",\n            });\n          });\n      });\n    },\n  },\n};\n</script>\n\n"],"sourceRoot":"src/views/clerk"}]}
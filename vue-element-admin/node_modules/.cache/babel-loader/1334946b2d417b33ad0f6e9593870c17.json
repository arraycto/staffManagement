n  },\n  //审核通过\n  updateAttendanceOk(id) {\n    return request({\n      url: `/staff/attendance/updateAttendanceOk/${id}`,\n      method: \"put\"\n    })\n  },\n  //不通过\n  updateAttendanceNo(id) {\n    return request({\n      url: `/staff/attendance/updateAttendanceNo/${id}`,\n      method: \"put\",\n      data: attendance\n    })\n  },\n\n}\n"]}]}
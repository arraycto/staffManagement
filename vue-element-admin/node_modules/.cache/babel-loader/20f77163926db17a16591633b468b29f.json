,CAIA;;AACA,WAAA,aAAA;AACA,KAPA;AAQA;AACA;AACA,IAAA,aAVA,2BAUA;AAAA;;AAAA,UAAA,IAAA,uEAAA,CAAA;AACA,WAAA,IAAA,GAAA,IAAA;AACA,MAAA,UAAA,CACA,aADA,CAEA,KAAA,IAFA,EAGA,KAAA,KAHA,EAIA,KAAA,iBAJA,EAMA,IANA,CAMA,UAAA,QAAA,EAAA;AACA;AACA;AACA,QAAA,KAAA,CAAA,IAAA,GAAA,QAAA,CAAA,IAAA,CAAA,cAAA;AACA,QAAA,KAAA,CAAA,KAAA,GAAA,QAAA,CAAA,IAAA,CAAA,KAAA;AACA,OAXA,EAYA,KAZA,CAYA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OAdA;AAeA;AA3BA,uCA4BA;AACA,SAAA,aAAA;AACA,GA9BA;AAxCA,CAAA","sourcesContent":["<template>\n  <div class=\"app-container\">\n    <!--查询表单-->\n    <el-form :inline=\"true\" class=\"demo-form-inline\">\n       \n      <el-form-item>\n        <el-select\n          clearable\n          v-model=\"attendanceQueryVo.status\"\n          placeholder=\"请选择奖惩类型\"\n        >\n          <el-option\n            v-for=\"item in options\"\n            :key=\"item.value\"\n            :label=\"item.label\"\n            :value=\"item.value\"\n          >\n          </el-option>\n        </el-select>\n      </el-form-item>\n\n      <el-button\n        type=\"primary\"\n        icon=\"el-icon-search\"\n        @click=\"getAttendance()\"\n        >查询</el-button\n      >\n      <el-button type=\"default\" @click=\"resetData()\">清空</el-button>\n    </el-form>\n    <el-table\n      :data=\"list\"\n      element-loading-text=\"数据加载中\"\n      fit\n      highlight-current-row\n    >\n      <el-table-column label=\"序号\" width=\"70\" align=\"center\">\n        <template slot-scope=\"scope\">{{\n          (page - 1) * limit + scope.$index + 1\n        }}</template>\n      </el-table-column>\n      <el-table-column prop=\"id\" sortable label=\"编号\" width=\"200\" />\n      <el-table-column\n        prop=\"status\"\n        sortable\n        label=\"类型\"\n        width=\"100\"\n      />\n      <el-table-column prop=\"time\" sortable label=\"日期\" width=\"200\" />\n      <el-table-column prop=\"audit\" label=\"状态\" />\n    </el-table>\n    <!-- 分页 -->\n    <el-pagination\n      :current-page=\"page\"\n      :page-size=\"limit\"\n      :total=\"total\"\n      style=\"padding: 30px 0; text-align: center\"\n      layout=\"total, prev, pager, next, jumper\"\n      @current-change=\"getAttendance\"\n    />\n  </div>\n</template>\n\n<script>\nimport attendance from \"@/api/attendance\";\nexport default {\n  data() {\n    return {\n      options: [\n        {\n          value: \"1\",\n          label: \"事假\",\n        },\n        {\n          value: \"2\",\n          label: \"迟到\",\n        },\n           {\n          value: \"3\",\n          label: \"早退\",\n        },\n        {\n          value: \"4\",\n          label: \"病假\",\n        },\n           {\n          value: \"5\",\n          label: \"旷工\",\n        },\n        {\n          value: \"6\",\n          label: \"休补\",\n        },\n      ],\n      list: null, //查询之后接口返回集合\n      page: 1, //当前页\n      limit: 6, //每页记录数\n      total: 0, //总记录数\n      attendanceQueryVo: {},\n    };\n  },\n  created() {\n    //页面渲染之前执行，一般用于调用methods中定义的方法\n    this.getAttendance();\n  },\n  methods: {\n    resetData() {\n      //清空的方法\n      //表单数据清空\n      this.attendanceQueryVo = {};\n      //查询所有\n      this.getAttendance();\n    },\n    //页面渲染之后执行，一般用于创建具体方法\n    //列表方法\n    getAttendance(page = 1) {\n      this.page = page;\n      attendance\n        .getAttendance(\n          this.page,\n          this.limit,\n          this.attendanceQueryVo\n        )\n        .then((response) => {\n          //请求成功\n          //response接口返回的数据\n          this.list = response.data.attendanceList;\n          this.total = response.data.total;\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    },\n    resetData() {\n      this.getAttendance();\n    },\n    \n  },\n};\n</script>\n\n"],"sourceRoot":"src/views/attendanceInfo"}]}